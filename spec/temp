<script type="text/javascript">

function show_jqsearch(){
if(document.getElementById('jq_search'))
	{
		$(loading_jqsearch).remove();
		document.getElementById('jq_search').style.display='';
    }
}

$(document).ready(function(){
	$(".btn-slide").click(function(){		$("#panel1").slideToggle("slow");
	    $('#pagenav_top').toggle();
		$('#to_rab_show').toggle();
		$('#subscribe_show').toggle();
	    $('#adw_top').toggle();
		if(this.value=='Изменить условия поиска'){this.value='Отмена'; document.getElementById('btn_new_search').style.display='none'; document.getElementById('old_select').style.display='none'; document.getElementById('btn_submit_search').style.display='';if(document.getElementById('searchstr_txt'))document.getElementById('searchstr_txt').style.display='none';}
		else {this.value='Изменить условия поиска'; document.getElementById('btn_new_search').style.display=''; document.getElementById('old_select').style.display=''; document.getElementById('btn_submit_search').style.display='none';if(document.getElementById('searchstr_txt'))document.getElementById('searchstr_txt').style.display='';}
		$(this).toggleClass("active"); return false;
	});
	$(".btn-slide2").click(function(){		$("#panel2").slideToggle("slow");
	    $('#pagenav_top').toggle();
		$('#to_rab_show').toggle();
		$('#subscribe_show').toggle();
	    $('#adw_top').toggle();
		if(this.value=='Новый поиск'){this.value='Отмена'; document.getElementById('subscribe_show').style.display='none'; document.getElementById('btn_change_search').style.display='none'; document.getElementById('old_select').style.display='none'; document.getElementById('btn_submit_search').style.display='none';if(document.getElementById('searchstr_txt'))document.getElementById('searchstr_txt').style.display='none';}
		else {this.value='Новый поиск'; document.getElementById('subscribe_show').style.display=''; document.getElementById('btn_change_search').style.display=''; document.getElementById('old_select').style.display=''; document.getElementById('btn_submit_search').style.display='none';if(document.getElementById('searchstr_txt'))document.getElementById('searchstr_txt').style.display='';}
		$(this).toggleClass("active"); return false;
	});

});
</script>

