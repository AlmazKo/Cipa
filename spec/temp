<script type="text/javascript">

if($.blockUI)
{
$.blockUI.defaults.css.border = '0px'; //убираем серую границу
$.blockUI.defaults.fadeIn = 100;  //ускоряем появление
$.blockUI.defaults.fadeOut = 100; //и исчезновение
$.blockUI.defaults.css.left = '1%'; //окно будет в центре
$.blockUI.defaults.css.top = '1%'; //окно будет в центре
}

function stop_invasion() {
	$('#invader').stop().stop().stop().stop().stop();
	$('#invader').css('left', 0);
	$('#invader').css('top', 0);
	$('#invader').hide();
	$('#show_me_the_secret').show();
	$('#hide_the_secret').hide();
}

    $(document).ready( function() {

	$('#hide_the_secret').click( function () {
	    stop_invasion();
	});

	$('#show_me_the_secret').click(function() {

		$('#hide_the_secret').show();
		$('#show_me_the_secret').hide();

		$('#invader').show()
		.animate({left:"1000px", top:"500px"}, 7000)
        .animate({left:"1000px", top:"100px"}, 3000)
        .animate({left:"10px",   top:"500px"}, 7000)
        .animate({left:"500px",  top:"500px"}, 3500)
        .animate({left:"500px",  top:"-70px"}, 7000)
		.queue( function() {
			stop_invasion();
			$('#indvader').dequeue();
		});
	});

    });
</script>


<script type="text/javascript">

$(document).ready(function() {
    //По этим кнопкам модальное окно закрывается
    $('.rotes_kreuz, .close_dialog').click(function() {
        $.unblockUI();
        return false;
    });


    //Эта кнопка будет вызывать наше окно
    $('#show_modal').click(function() {
        var dialog_width = $('#modal_dialog').css("width");
        $.blockUI({message: $('#modal_dialog'), css: {width: dialog_width}});
    });


    //По кнопке "Да" будет отправляться форма
    $('#modal_dialog #yes').click(function() {
        $('#modal_dialog form').submit();
    });
});

function showmodal(modal_div) {
	    if(!modal_div) modal_div = "#modal_dialog";
        var dialog_width = $(modal_div).css("width");
        $.blockUI({message: $(modal_div), css: {width: dialog_width}});
}

</script>

    <script src="http://api-maps.yandex.ru/1.1/?key=AO8lMksBAAAAiBnlFAIAm_va7hpvGPyp-AaNpuTIAPXRBboAAAAAAAAAAADbIfju5y4ijnflm3KVjeW6szBx6A==" type="text/javascript"> </script>
    <script type="text/javascript">
        var map;
        var div_id;
        var lat;
        var lng;
        var txt;
	    var s_id;

        function TextualZoomControl (s_id) {
            this.element = document.createElement("DIV");
            this.element.style.position = 'absolute';
            this.element.style.zIndex = '1000';
            this.element.id=s_id;
        }

        TextualZoomControl.prototype = {
            onAddToMap: function (map, position) {
                this.map = map;
                this.position = position || new YMaps.ControlPosition(YMaps.ControlPosition.TOP_RIGHT, new YMaps.Size(10, 10));
                this._init();
            },

            onRemoveFromMap: function () {
                if (this.element.parentNode) {
                    this.map.getContainer().removeChild(this.element);
                }
                this.map = null;
            },

           _init: function (map) {
                var zoomInDiv = document.createElement("div"),
                    zoomOutDiv = document.createElement("div"),
                    GoogleDiv = document.createElement("div"),
                    _this = this;

                this._setButtonStyle(zoomInDiv);
                this.element.appendChild(zoomInDiv);
                zoomInDiv.appendChild(document.createTextNode("Скрыть"));

                zoomInDiv.onclick  = function () {
                    this.parentNode.parentNode.style.display='none';
                    _this.map.destructor();
				var onmap =_this.element.id.split('_');
			     document.getElementById('o_'+ onmap[1]).style.display='';
                };


                this._setButtonStyle(zoomOutDiv);
                this.element.appendChild(zoomOutDiv);
                zoomOutDiv.appendChild(document.createTextNode("Увеличить"));


                zoomOutDiv.onclick  = function () {
                    this.parentNode.parentNode.style.display='none';
                    _this.map.destructor();
				var onmap =_this.element.id.split('_');
                window.open('http://www.cian.ru/onmap.php?obj='+onmap[0] + '&id_obj=' + onmap[1]);
                };


                this._setButtonStyle(GoogleDiv);
                this.element.appendChild(GoogleDiv);
                GoogleDiv.appendChild(document.createTextNode("на Google"));

                GoogleDiv.onclick  = function () {
                this.parentNode.parentNode.style.display='none';
                _this.map.destructor();
				var onmap =_this.element.id.split('_');
                window.open('http://www.cian.ru/onmap.php?g=1&obj='+onmap[0] + '&id_obj=' + onmap[1]);
                };


                this.position.apply(this.element);
                this.map.getContainer().appendChild(this.element);
            },

            _setButtonStyle: function (button) {
                style = {textDecoration: 'underline',
                         color: 'black',
                         textDecoration: 'none',
                         backgroundColor: '#C0C0C0',
                         font: 'small Arial',
                         border: '2px solid black',
                         padding: '2px',
                         marginBottom: '3px',
                         textAlign: 'center',
                         width: '6em',
                         cursor: 'pointer'};
                for (var k in style) {
                    button.style[k] = style[k];
                }
            }
        }

  var route_set;
  var incr;
function init(div_id,lat,lng,txt,obj,route_set,incr) {
            if(!incr) incr=16;
            YMaps.load(function () { init_map(div_id,lat,lng,txt,obj,route_set,incr) });
        }

        function init_map(div_id,lat,lng,txt,obj,route_set,incr) {
      	document.getElementById('o_'+ div_id).style.display='none';
        map = new YMaps.Map(document.getElementById('m_'+ div_id));
        document.getElementById('m_'+ div_id).style.display='block';
            map.setCenter(new YMaps.GeoPoint(lat, lng), incr);
            var point = new YMaps.GeoPoint(lat, lng);
            var placemark = new YMaps.Placemark(point);
            txt = txt.split(' ');
            var distance_txt = '';
            var metro_txt = '';
            var mcad_txt = '';
            var point2 = new YMaps.GeoPoint(txt[0],txt[1]);

            if( (route_set==1 || route_set==3) && txt[0]>0 && txt[1]>0 )
            	{
					var metro_txt = "До&nbsp;<span class=\"link\" onclick=\"map.panTo(new YMaps.GeoPoint("+txt[0]+","+txt[1]+"));\">метро</span><br>";
          		  	var distance = point.distance(point2);
					if(distance) distance_txt = "пешком&nbsp;по&nbsp;прямой&nbsp;" + "&nbsp;&asymp;&nbsp;" + (distance/(6000/60)).toFixed(0) + "мин&nbsp;(" + YMaps.humanDistance(distance)+ ")<BR>";
                }

//            if( (route_set==1 || route_set==2) && txt[0]>0 && txt[1]>0 )
//            	{
            		        var router = new YMaps.Router([point2, new YMaps.GeoPoint(lat,lng)],[1],{ viewAutoApply: false });
       						YMaps.Events.observe(router, router.Events.Success, function() {

							if(route_set==2)var fullDistance = (router.getDistance()/1000).toFixed(0);
							else var fullDistance = (router.getDistance()/1000).toFixed(1);
							var fullTime = (router.getDuration()/60).toFixed(0);

            				if(route_set==2) {metro_txt=''; distance_txt = "От&nbsp;<a href=\"javascript: map.panTo(new YMaps.GeoPoint("+txt[0]+","+txt[1]+"));\">МКАД</a> &asymp; " + fullDistance + 'км';}
            				else distance_txt = distance_txt + 'на автомобиле без пробок &asymp; ' + fullTime + 'мин (' + fullDistance + 'км)';

      						if(distance_txt) placemark.setBalloonContent(metro_txt + distance_txt);
	            			map.addOverlay(placemark);
    		    			if(distance_txt) placemark.openBalloon();

								});
//            	}

	            			map.addOverlay(placemark);


//                if(distance_txt) placemark.setBalloonContent(metro_txt + distance_txt);
//	            map.addOverlay(placemark);
//    		    if(distance_txt) placemark.openBalloon();

            

            map.addControl(new YMaps.ToolBar());
            //map.addControl(new YMaps.TypeControl());
            map.addControl(new YMaps.Zoom());
            map.addControl(new YMaps.ScaleLine());
            if(div_id && obj) map.addControl(new TextualZoomControl(obj + '_'+ div_id));

            
        }
    </script>

<script language="javascript">
var Left = 400;
var Top = 250;

function scroll_control(block_id)
{
if(!block_id)block_id="login_new";
if(document.getElementById("doc").scrollTop > 0){document.getElementById(block_id).style.top = document.getElementById("doc").scrollTop + Top;}
else {document.getElementById(block_id).style.top = Top;}
}
</script>


