<script type="text/javascript">
function add_smart_cart_val(source,cart_id,id_object,mode)
{
	$.post("/js/smart_cart_val.php", {cart_id:cart_id,id_object:id_object,mode:mode}, function(result){
  	result = eval(result);

  	if(result[0]>0)
  	{
		source_r= $(source).parent();
		$(source).remove();

		if(result[1]=='added'){mode='remove'; caption='удалить'; color='red';}
		if(result[1]=='removed'){mode='add'; caption='добавить'; color='green';}

		if(result[1]=='added'){src='/images/added.gif'; caption='удалить';}
		if(result[1]=='removed'){src='/images/fav_s.png'; caption='добавить';}


		//html = '<span onclick="add_smart_cart_val(this,' + cart_id + ',' + id_object + ',\'' + mode + '\');" style="color:' + color + ';text-decoration:underline;cursor:pointer;">' + caption + '</span>';
		html = '<input type="image" onclick="add_smart_cart_val(this,' + cart_id + ',' + id_object + ',\'' + mode + '\');" src="' + src + '" alt="' + caption + '" title="' + caption + '" style="color:' + color + ';text-decoration:underline;cursor:pointer;"></span>';
		$(source_r).append(html);
	}
	else
	{
		alert(result[1]);
	}

	});
}
</script><script type="text/javascript" src="/js/jquery.blockUI.js"></script>

